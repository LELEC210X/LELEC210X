	component packet_presence_detection_tb_gen is
		port (
			clock_clk    : in  std_logic                     := 'X';             -- clk
			reset_reset  : in  std_logic                     := 'X';             -- reset
			sink_data    : in  std_logic_vector(23 downto 0) := (others => 'X'); -- data
